{
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "arrowParens": "always",
  "trailingComma": "es5",
  "semi": true,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxBracketSameLine": false,
  "editor.formatOnSave": true,
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-organize-attributes"]
}
