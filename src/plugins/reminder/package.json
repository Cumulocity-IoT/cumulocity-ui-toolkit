{
  "name": "cumulocity-reminder-plugin",
  "version": "1.4.1",
  "inherits": [],
  "description": "",
  "scripts": {
    "start": "ng serve -u https://psfactory.eu-latest.cumulocity.com",
    "start:cp": "npm start -- --shell cockpit",
    "start:cc": "npm start -- --shell clean-cockpit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "ng build",
    "postbuild": "node -e \"require('fs').rename('dist/plugin.zip', 'dist/${npm_package_name}_${npm_package_version}.zip', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "deploy": "ng deploy",
    "test": "ng test"
  },
  "keywords": [
    "cumulocity",
    "cumulocity-package",
    "cumulocity-iot",
    "cumulocity-1020",
    "cumulocity-ui-plugin",
    "reminder"
  ],
  "author": "Dirk Peter",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cumulocity-IoT/cumulocity-reminder-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/Cumulocity-IoT/cumulocity-reminder-plugin/issues"
  },
  "homepage": "https://github.com/Cumulocity-IoT/cumulocity-reminder-plugin#readme",
  "dependencies": {
    "ngx-moment": "^6.0.2"
  },
  "devDependencies": {
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lodash": "^4.4.0",
    "rimraf": "^5.0.5"
  }
}
