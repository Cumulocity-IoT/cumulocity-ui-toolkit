{
  "name": "ps-ui-toolkit",
  "version": "0.0.0",
  "author": "Cumulocity UI Guild, @dirk-peter-c8y",
  "license": "Apache-2.0",
  "private": true,
  "config": {},
  "keywords": [
    "cumulocity",
    "cumulocity-package",
    "cumulocity-iot",
    "cumulocity-ui-plugin",
    "cumulocity-ui-widget",
    "cumulocity-ui-guild"
  ],
  "scripts": {
    "build": "run-s build:*",
    "test": "npm-run-all -p test:*",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --quiet --fix",
    "--pre/post-----------": "--------------------",
    "prebuild": "npm run clean",
    "--tools--------------": "--------------------",
    "generate:scripts": "node tools/generate-scripts.js",
    "generate:assets": "node tools/generate-assets.js",
    "clean": "rimraf dist",
    "--shortcuts----------": "--------------------",
    "_serve": "from-env ng serve -u %C8Y_BASEURL --shell %C8Y_SHELL_TARGET",
    "_test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "_postbuild": "node tools/postbuild.js",
    "--locals-------------": "--------------------",
    "ng": "ng",
    "eslint": "eslint",
    "--e2e----------------": "--------------------",
    "e2e:run": "npx --workspace=test -- cypress run --browser chrome",
    "e2e:open": "cypress open",
    "e2e:run:favorites-manager": "npm run e2e:run -- --config-file config/favorites-manager.config.ts",
    "e2e:run:reminder": "npm run e2e:run -- --config-file config/reminder.config.ts",
    "e2e:run:kpi-widget": "npm run e2e:run -- --config-file config/kpi-widget.config.ts",
    "e2e:run:energy-consumption-widget": "npm run e2e:run -- --config-file config/energy-consumption-widget.config.ts",
    "e2e:run:release-notes": "npm run e2e:run -- --config-file config/release-notes.config.ts",
    "e2e:run:operations-widget": "npm run e2e:run -- --config-file config/operations-widget.config.ts",
    "e2e:run:tenant-option-management": "npm run e2e:run -- --config-file config/tenant-option-management.config.ts",
    "e2e:open:favorites-manager": "npx --workspace=test -- cypress open --config-file config/favorites-manager.config.ts",
    "e2e:open:release-notes": "npx --workspace=test -- cypress open --config-file config/release-notes.config.ts",
    "--generated----------": "--------------------",
    "serve:reminder": "npm run _serve plugin.reminder",
    "build:reminder": "ng build plugin.reminder",
    "test:reminder": "npm run _test plugin.reminder",
    "serve:kpi-widget": "npm run _serve plugin.kpi-widget",
    "build:kpi-widget": "ng build plugin.kpi-widget",
    "test:kpi-widget": "npm run _test plugin.kpi-widget",
    "serve:release-notes": "npm run _serve plugin.release-notes",
    "build:release-notes": "ng build plugin.release-notes",
    "test:release-notes": "npm run _test plugin.release-notes",
    "serve:energy-consumption-widget": "npm run _serve plugin.energy-consumption-widget",
    "build:energy-consumption-widget": "ng build plugin.energy-consumption-widget",
    "test:energy-consumption-widget": "npm run _test plugin.energy-consumption-widget",
    "serve:favorites": "npm run _serve plugin.favorites",
    "build:favorites": "ng build plugin.favorites",
    "test:favorites": "npm run _test plugin.favorites",
    "serve:tenant-option-management": "npm run _serve plugin.tenant-option-management",
    "build:tenant-option-management": "ng build plugin.tenant-option-management",
    "test:tenant-option-management": "npm run _test plugin.tenant-option-management",
    "serve:operations-widget": "npm run _serve plugin.operations-widget",
    "build:operations-widget": "ng build plugin.operations-widget",
    "test:operations-widget": "npm run _test plugin.operations-widget"
  },
  "workspaces": [
    "packages/*",
    "test"
  ],
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.10",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@c8y/bootstrap": "1021.22.146",
    "@c8y/client": "1021.22.146",
    "@c8y/ngx-components": "1021.22.146",
    "@c8y/style": "1021.22.146",
    "dotenv": "^17.0.1",
    "ngx-bootstrap": "18.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/eslint-plugin-template": "^19.2.1",
    "@angular-eslint/template-parser": "^19.2.1",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@c8y/devkit": "1021.22.146",
    "@c8y/options": "1021.22.146",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "from-env": "^1.1.4",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "rimraf": "^5.0.10",
    "typescript": "~5.5.2",
    "wait-on": "^7.0.1"
  },
  "overrides": {
    "globals": "14.0.0"
  }
}
