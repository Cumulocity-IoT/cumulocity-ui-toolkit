{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "controlcenter": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less",
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "src/apps/controlcenter",
      "sourceRoot": "src/apps/controlcenter/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@c8y/devkit:build",
          "options": {
            "outputPath": "dist/controlcenter",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "src/apps/controlcenter/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "src/apps/controlcenter/src/favicon.ico",
              "src/apps/controlcenter/src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/"
              ]
            },
            "styles": [
              "src/apps/controlcenter/src/styles/style.less",
              "node_modules/prunecluster/dist/LeafletStyleSheet.css"
            ],
            "scripts": [
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/prunecluster/dist/PruneCluster.js"
            ],
            "aot": false,
            "main": "src/apps/controlcenter/src/main.ts",
            "buildOptimizer": false,
            "allowedCommonJsDependencies": [
              "regenerator-runtime/runtime",
              "@cordobo/qrcode",
              "zone.js/dist/zone",
              "chroma-js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "buildOptimizer": false
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@c8y/devkit:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "controlcenter:build:production"
            },
            "development": {
              "buildTarget": "controlcenter:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@c8y/devkit:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "src/apps/controlcenter/tsconfig.spec.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "src/apps/controlcenter/src/favicon.ico",
              "src/apps/controlcenter/src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/"
              ]
            },
            "styles": [
              "src/apps/controlcenter/src/styles/style.less"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@c8y/devkit:deploy",
          "configurations": {
            "production": {
              "buildTarget": "controlcenter:build:production"
            },
            "development": {
              "buildTarget": "controlcenter:build:development"
            }
          },
          "defaultConfiguration": "production"
        },
        "compile-i18n": {
          "builder": "@c8y/devkit:compile-i18n"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
